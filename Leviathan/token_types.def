X(LBrace = 1 << 4)
X(RBrace = 1 << 5)

X(String = 1 << 6)
X(Keyword = 1 << 7)
X(Symbol = 1 << 8)
X(Number = 1 << 9)
X(Regex = 1 << 10)

X(Quote = 1 << 11)
X(Unquote = 1 << 12)
X(SyntaxQuote = 1 << 13)
X(Splice = 1 << 14)
X(TypeOp = 1 << 15)

X(ReaderCommentStart = 1 << 16)
X(ReaderMacro = 1 << 17)
X(AnonFnStart = 1 << 18)
X(SetStart = 1 << 19)
X(Var = 1 << 20)

X(Comma = 1 << 21)
X(Spaces = 1 << 22)
X(Newline = 1 << 23)

X(Comment = 1 << 24)

X(FileBegin = 1 << 25)
